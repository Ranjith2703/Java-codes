import java.util.*;

public class Main {
            public static int gcd(int a,int b){
            while(b!=0){
                int t=b;
                b=a%b;
                a=t;
            }
            return a;
        }
    public static void main(String args[]) {
        // Your Code Here
        Scanner n=new Scanner(System.in);
        int a=n.nextInt();
        int b=n.nextInt();

        System.out.println(a*b/gcd(a,b));
    }
}
//GCD
import java.util.*;

public class Main {
    public static void main(String args[]) {
        // Your Code Here
        Scanner n=new Scanner(System.in);
        int a=n.nextInt();
        int b=n.nextInt();
        while(b>0){
            int t=b;
            b=a%b;
            a=t;
        }
        System.out.println(a);
    }
}
