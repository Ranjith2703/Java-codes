import java.util.*;

public class Main {
    public static void main(String args[]) {
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        n+=1;
        if(n<=1) n=2;
        for(int i=2;i<n;i++){
            if(n%i==0) n+=1;
        }
        System.out.println(n);
    }
    
}
